{
  "name": "paks-web",
  "description": "Source project for pakscontractors.com.au",
  "version": "1.0.0",
  "private": true,
  "author": "Sam Ilic",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public && rimraf .cache",
    "clean-packages": "rimraf package-lock.json && rimraf node_modules",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "upgrade": "ncu -u && npm run clean-packages && npm install"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.13.2",
    "classnames": "2.3.1",
    "gatsby": "3.11.1",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-canonical-urls": "3.11.0",
    "gatsby-plugin-react-helmet": "4.11.0",
    "gatsby-plugin-sass": "4.11.0",
    "gatsby-plugin-sharp": "3.11.0",
    "gatsby-plugin-typescript": "3.11.0",
    "gatsby-remark-copy-linked-files": "4.8.0",
    "gatsby-remark-images": "5.8.0",
    "gatsby-remark-prismjs": "5.8.0",
    "gatsby-remark-responsive-iframe": "4.8.0",
    "gatsby-remark-smartypants": "4.8.0",
    "gatsby-source-filesystem": "3.11.0",
    "gatsby-transformer-json": "3.11.0",
    "gatsby-transformer-remark": "4.8.0",
    "gatsby-transformer-sharp": "3.11.0",
    "node-sass": "6.0.1",
    "prism-themes": "1.8.0",
    "prismjs": "1.24.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "6.1.0",
    "typescript": "4.3.5"
  },
  "devDependencies": {
    "@types/classnames": "2.3.0",
    "@types/node": "16.6.1",
    "@types/react": "17.0.18",
    "@types/react-dom": "17.0.9",
    "@types/react-helmet": "6.1.2",
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "gh-pages": "3.2.3",
    "prettier": "2.3.2",
    "rimraf": "3.0.2"
  }
}
