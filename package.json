{
  "name": "paks-web",
  "description": "Source project for pakscontractors.com.au",
  "version": "1.0.0",
  "private": true,
  "author": "Sam Ilic",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public && rimraf .cache",
    "clean-packages": "rimraf package-lock.json && rimraf node_modules",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "upgrade": "ncu -u && npm run clean-packages && npm install"
  },
  "engines": {
    "node": "14.x"
  },
  "dependencies": {
    "babel-plugin-styled-components": "2.0.7",
    "classnames": "2.3.1",
    "gatsby": "4.12.1",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-canonical-urls": "4.12.1",
    "gatsby-plugin-react-helmet": "5.12.1",
    "gatsby-plugin-sass": "5.12.1",
    "gatsby-plugin-sharp": "4.12.1",
    "gatsby-plugin-typescript": "4.12.1",
    "gatsby-remark-copy-linked-files": "5.12.1",
    "gatsby-remark-images": "6.12.1",
    "gatsby-remark-prismjs": "6.12.1",
    "gatsby-remark-responsive-iframe": "5.12.1",
    "gatsby-remark-smartypants": "5.12.1",
    "gatsby-source-filesystem": "4.12.1",
    "gatsby-transformer-json": "4.12.1",
    "gatsby-transformer-remark": "5.12.1",
    "gatsby-transformer-sharp": "4.12.1",
    "sass": "1.50.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.27.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-helmet": "6.1.0",
    "typescript": "4.6.3"
  },
  "devDependencies": {
    "@types/classnames": "2.3.0",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@types/react-helmet": "6.1.5",
    "@typescript-eslint/eslint-plugin": "5.19.0",
    "@typescript-eslint/parser": "5.19.0",
    "eslint": "8.13.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.0.0",
    "gh-pages": "3.2.3",
    "prettier": "2.6.2",
    "rimraf": "3.0.2"
  }
}
