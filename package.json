{
  "name": "paks-web",
  "description": "Source project for pakscontractors.com.au",
  "version": "1.0.0",
  "private": true,
  "author": "Sam Ilic",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public && rimraf .cache",
    "clean-packages": "rimraf package-lock.json && rimraf node_modules",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "upgrade": "ncu -u && npm run clean-packages && npm install"
  },
  "engines": {
    "node": "18.x"
  },
  "dependencies": {
    "babel-plugin-styled-components": "2.1.4",
    "classnames": "2.5.1",
    "gatsby": "5.14.0",
    "gatsby-image": "3.11.0",
    "gatsby-plugin-canonical-urls": "5.14.0",
    "gatsby-plugin-react-helmet": "6.14.0",
    "gatsby-plugin-sass": "6.14.0",
    "gatsby-plugin-sharp": "5.14.0",
    "gatsby-plugin-typescript": "5.14.0",
    "gatsby-remark-copy-linked-files": "6.14.0",
    "gatsby-remark-images": "7.14.0",
    "gatsby-remark-prismjs": "7.14.0",
    "gatsby-remark-responsive-iframe": "6.14.0",
    "gatsby-remark-smartypants": "6.14.0",
    "gatsby-source-filesystem": "5.14.0",
    "gatsby-transformer-json": "5.14.0",
    "gatsby-transformer-remark": "6.14.0",
    "gatsby-transformer-sharp": "5.14.0",
    "prism-themes": "1.9.0",
    "prismjs": "1.29.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "sass": "1.81.0",
    "typescript": "5.7.2"
  },
  "devDependencies": {
    "@types/classnames": "2.3.4",
    "@types/node": "22.10.1",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "prettier": "3.4.1",
    "rimraf": "6.0.1"
  }
}
