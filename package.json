{
  "name": "paks-web",
  "description": "Source project for pakscontractors.com.au",
  "version": "1.0.0",
  "private": true,
  "author": "Sam Ilic",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public && rimraf .cache",
    "clean-packages": "rimraf package-lock.json && rimraf node_modules",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "upgrade": "ncu -u && npm run clean-packages && npm install"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.12.0",
    "classnames": "2.2.6",
    "gatsby": "3.0.1",
    "gatsby-image": "2.11.0",
    "gatsby-plugin-canonical-urls": "3.0.0",
    "gatsby-plugin-react-helmet": "4.0.0",
    "gatsby-plugin-sass": "4.0.0",
    "gatsby-plugin-sharp": "3.0.0",
    "gatsby-plugin-typescript": "3.0.0",
    "gatsby-remark-copy-linked-files": "3.0.0",
    "gatsby-remark-images": "4.0.0",
    "gatsby-remark-prismjs": "4.0.0",
    "gatsby-remark-responsive-iframe": "3.0.0",
    "gatsby-remark-smartypants": "3.0.0",
    "gatsby-source-filesystem": "3.0.0",
    "gatsby-transformer-json": "3.0.0",
    "gatsby-transformer-remark": "3.0.0",
    "gatsby-transformer-sharp": "3.0.0",
    "node-sass": "5.0.0",
    "prism-themes": "1.5.0",
    "prismjs": "1.23.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-helmet": "6.1.0",
    "typescript": "4.2.2"
  },
  "devDependencies": {
    "@types/classnames": "2.2.11",
    "@types/node": "14.14.31",
    "@types/react": "17.0.2",
    "@types/react-dom": "17.0.1",
    "@types/react-helmet": "6.1.0",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "7.21.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "gh-pages": "3.1.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2"
  }
}
